{
  "name": "nombredelproyecto",
  "version": "1.6.2",
  "description": "Descripcion del proyecto",
  "author": "autor(es)",
  "homepage": "http://www.tudominio.io",
  "copyright": "",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start-shared": "ng serve --host [ip de la micro]",
    "hmr": "ng serve  --configuration hmr",
    "build": "ng build --prod && cd dist/assets && del settings.prod.json",
    "build-osx": "ng build --prod && rm dist/assets/settings.prod.json",
    "build:dev": "ng build --configuration dev && cd dist/assets && del settings.prod.json",
    "build:dev-osx": "ng build --configuration dev && rm dist/assets/settings.prod.json",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "compodoc": "compodoc -p tsconfig.json",
    "compodoc-serve": "compodoc -s",
    "json-start": "nodemon server.js",
    "json-start-shared": "nodemon server.js --host [ip de la micro] --port 3200",
    "dev": "./node_modules/.bin/npm-run-all --parallel start json-start",
    "dev:hmr": "./node_modules/.bin/npm-run-all --parallel hmr json-start",
    "dev:shared": "./node_modules/.bin/npm-run-all --parallel start-shared json-start-shared"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && ng lint"
    }
  },
  "private": true,
